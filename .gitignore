*.class
*.jar
*.war
*.ear
*.iml
*.ipr
*.iws
*.log
*.javadoc
/.idea/
/.settings/
/.classpath
/.project
/.factorypath
/target/
/bin/

# Go
/bin/
/pkg/
*.exe
*.test
*.out
*.log
*.mod
*.sum

# VSCode
.vscode/

